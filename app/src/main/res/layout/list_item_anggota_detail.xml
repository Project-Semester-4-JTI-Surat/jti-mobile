<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="10dp">
    <TextView
        android:id="@+id/txt_anggota"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:text="Anggota"
        android:textStyle="bold"
        />
    <TextView
        android:id="@+id/txt_nim"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="NIM Anggota"
        android:textStyle="bold"
        android:layout_below="@id/txt_anggota"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:id="@+id/tv_nim"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="NIM"
        android:layout_below="@+id/txt_nim"
        />
    <TextView
        android:id="@+id/txt_nama"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="Nama Anggota"
        android:layout_toEndOf="@id/txt_nim"
        android:textStyle="bold"
        android:layout_below="@id/txt_anggota"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:id="@+id/tv_nama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="NAMA"
        android:layout_toEndOf="@+id/tv_nim"
        android:layout_below="@id/txt_nama"/>
    <TextView
        android:id="@+id/txt_prodi"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="Prodi Anggota"
        android:layout_below="@id/tv_nim"
        android:textStyle="bold"
        android:layout_marginTop="7dp"
        />
    <TextView
        android:id="@+id/tv_prodi"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="PRODI"
        android:layout_below="@id/txt_prodi"
        />
    <TextView
        android:id="@+id/txt_nohp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="No HP Anggota"
        android:layout_below="@id/tv_nama"
        android:layout_toEndOf="@+id/txt_prodi"
        android:textStyle="bold"
        android:layout_marginTop="7dp"
        />
    <TextView
        android:id="@+id/tv_nohp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:textColor="@color/black"
        android:text="NO HP"
        android:layout_toEndOf="@+id/tv_prodi"
        android:layout_below="@id/txt_nohp"/>

</RelativeLayout>